  <body>
    

    <div class="container-narrow">
    	<h2 class="form-signin-heading">Contact <%= link_to image_tag("greensmilen.jpg", alt: "smile") %></h2>
    	<div class="touch">
	        <p>If you would like to get in touch, feel free to email me below or hit me up on Twitter.</p>
	    </div>

      <form class="form-signin">

        <input type="text" class="input-block-level" placeholder="First name"> 
        <input type="text" class="input-block-level" placeholder="Last name">

        <input type="text" class="input-block-level" placeholder="Email address">
        <textarea type="text" class="input-block-level" placeholder="How can I assist you"></textarea>
        
        <button class="btn btn-large btn-primary" type="submit">Submit</button>
      </form>

    </div> <!-- /container -->
 

